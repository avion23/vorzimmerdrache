      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "webhookId": "sms-minimal"
    },
    {
      "parameters": {
        "jsCode": "return [{json: {message: 'Hello from n8n'}}];"
      },
      "name": "Code",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2
    },
    {
      "parameters": {
        "respondWith": "text",
        "responseBody": "OK"
      },
      "name": "Response",
      "type": "n8n-nodes-base.respondToWebhook",
