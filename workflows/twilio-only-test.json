{
  "name": "Twilio Only Test",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "twilio-only",
        "responseMode": "responseNode"
      },
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "webhookId": "twilio-only"
    },
    {
      "parameters": {
        "from": "={{ $env.TWILIO_PHONE_NUMBER }}",
        "to": "+491711234567",
        "body": "Test from n8n"
      },
      "name": "Twilio",
      "type": "n8n-nodes-base.twilio",
      "typeVersion": 1,
      "credentials": {
        "twilioApi": {
          "id": "FLTo0keeCh2osFRj",
          "name": "Twilio API"
        }
      }
    },
    {
      "parameters": {
        "respondWith": "text",
        "responseBody": "OK"
      },
      "name": "Response",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1
    }
  ],
  "connections": {
    "Webhook": {"main": [[{"node": "Twilio"}]]},
    "Twilio": {"main": [[{"node": "Response"}]]}
  },
  "settings": {"executionOrder": "v1"}
}
