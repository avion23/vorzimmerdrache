{
  "name": "vorzimmerdrache-enrichment",
  "version": "1.0.0",
  "description": "Solar lead enrichment module with address validation and solar potential estimation",
  "main": "integrations/enrichment/orchestrator.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "enrich": "node -e \"require('./integrations/enrichment/example.js')()\""
  },
  "keywords": [
    "solar",
    "enrichment",
    "geocoding",
    "lead-scoring"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "express": "^5.2.1",
    "jest": "^29.7.0",
    "nock": "^13.5.1",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "integrations/**/*.js",
      "!integrations/**/*.test.js",
      "!integrations/**/*.example.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
