{
  "name": "vorzimmerdrache-enrichment",
  "version": "1.0.0",
  "description": "Solar lead enrichment module with address validation and solar potential estimation",
  "main": "integrations/enrichment/orchestrator.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "enrich": "node -e \"require('./integrations/enrichment/example.js')()\"",
    "migrate:sheets-to-postgres": "node scripts/migrate-sheets-to-postgres.js",
    "sync:start": "node integrations/crm/sheets-postgres-sync.js daemon",
    "sync:once": "node integrations/crm/sheets-postgres-sync.js once",
    "sync:baserow:daemon": "node integrations/crm/baserow-sync.js daemon",
    "sync:baserow:once": "node integrations/crm/baserow-sync.js once",
    "rollback:to-sheets": "./scripts/rollback-to-sheets.sh",
    "benchmark": "./scripts/benchmark.sh",
    "migrate:db": "docker exec -i postgres psql -U n8n -d n8n < migrations/005_add_performance_indexes.sql",
    "setup:baserow": "./scripts/setup-baserow.sh"
  },
  "keywords": [
    "solar",
    "enrichment",
    "geocoding",
    "lead-scoring"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "google-spreadsheet": "^4.1.0",
    "ioredis": "^5.9.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "express": "^5.2.1",
    "jest": "^29.7.0",
    "nock": "^13.5.1",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "integrations/**/*.js",
      "!integrations/**/*.test.js",
      "!integrations/**/*.example.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
